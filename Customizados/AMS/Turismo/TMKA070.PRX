	Local nPos				:= 0	// Linha do item padrÃ£o
	Local nPosVerificador	:= 0	// PosiÃ§Ã£o do campo verificador
	Local lFaltaObrigatorio	:= .F.	// Indicador de campo obrigatÃ³rio nÃ£o preenchido
