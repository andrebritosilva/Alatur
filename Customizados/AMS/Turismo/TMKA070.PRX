				Replace &("SU5->" + A70AGBCampo(cTipo)) With cCodAGB
				&("M->" + A70AGBCampo(cTipo)) := cCodAGB
				SU5->(MsUnLock())  
				lAdicionado := .T.
			//ÃšÃ„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Â¿
			//Â³Se a variÃ¡vel lPrefSU5 for .T., significa que estÃ¡ em modo    Â³
			//Â³de compatibilidade. Por isso o conteÃºdo de AGB serÃ¡ atualizadoÂ³
			//Â³com os dados do cabeÃ§alho.                                    Â³
			//Ã€Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã„Ã™				
			ElseIf lPrefSU5 .And. (Type("lTk070Auto") <> "U" .Or. lTk070Auto) .And. !Empty(cCodAGB) 
				AGB->(DbSelectArea("AGB"))
				AGB->(DbSetOrder(2))
				If AGB->(DbSeek( xFilial("AGB") + cCodAGB ))
					RecLock("AGB",.F.)
					Replace AGB->AGB_TELEFO With cTel
					Replace AGB->AGB_DDI	With cDDI
					Replace AGB->AGB_DDD	With cDDD
					AGB->(MsUnLock())
				EndIf
				
				For nCount := 1 To Len(aColsAGB)
					If aColsAGB[nCount][nPosCodigo] == cCodAGB
						aColsAGB[nCount][nPosTel] := cTel
						aColsAGB[nCount][nPosDDI] := cDDI
						aColsAGB[nCount][nPosDDD] := cDDD
					EndIf
				Next
				lAdicionado := .T.
			EndIf
		EndIf
	EndIf
	
	SU5->(RestArea(aAreaSU5))
	AGB->(RestArea(aAreaAGB))
Return lAdicionado

