<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"	elementFormDefault="qualified">
	<xs:include schemaLocation="../totvsmsg.xsd"></xs:include>
	<xs:include schemaLocation="../types/Address_1_000.xsd"></xs:include>
	<xs:include schemaLocation="../types/CommunicationInformation_1_000.xsd"></xs:include>
	<xs:include schemaLocation="../types/ContactInformation_1_000.xsd"></xs:include>
	<xs:include schemaLocation="../types/GovernmentalInformation_1_000.xsd"></xs:include>
	<xs:include schemaLocation="../types/ListOfInternalId_1_000.xsd"></xs:include>
	<xs:complexType name="BusinessContentType">
		<xs:sequence>
			<xs:element name="CompanyId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Empresa</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BranchId" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Filial</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompanyInternalId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>InternalId da chave completa de empresa do produto</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Código do Cliente/Fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.cod-emitente</Field>
						   <Required>sim</Required>
						   <Type>integer</Type>
						   <Length>9</Length>
						   <Description>
								Código do Cliente/Fornecedor
								Formato do Campo: >>>>>>>>9
						   </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
			</xs:element>
			<xs:element name="StoreId" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
				  <xs:documentation>Loja</xs:documentation>
				</xs:annotation>
			</xs:element>     
			<xs:element name="InternalId" type="xs:string" maxOccurs="1" minOccurs="0">
				<xs:annotation>
					<xs:documentation>InternalId da entidade</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShortName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nome Abreviado</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.nome-abrev</Field>
						   <Required>sim</Required>
						   <Type>string</Type>
						   <Length>12</Length>
						   <Description>
								Nome Abreviado do Cliente/Fornecedor
						   </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
			</xs:element>
			<xs:element name="Name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Nome do Cliente/Fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.nome-emit</Field>
						   <Required>sim</Required>
						   <Type>string</Type>
						   <Length>60</Length>
						   <Description>
								Nome Completo do Cliente/Fornecedor
						   </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="40"></xs:maxLength>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo do Registro: Customer Cliente, Vendor Fornecedor, Both Ambos</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.identific</Field>
						   <Required>sim</Required>
						   <Type>integer</Type>
						   <Length>>9</Length>
						   <Description>
								Identifica se o emitente é apenas Cliente, apenas Fornecedor ou Ambos
								 1 – Cliente / 2 – Fornecedor / 3 – Ambos
						   </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Customer"><xs:annotation><xs:documentation>Cliente</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Vendor"><xs:annotation><xs:documentation>Fornecedor</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Both"><xs:annotation><xs:documentation>Ambos</xs:documentation></xs:annotation></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EntityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicador da natureza do Cliente/Fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.natureza</Field>
						   <Required>sim</Required>
						   <Type>integer</Type>
						   <Length>>9</Length>
						   <Description>
								Identifica se o emitente é Pessoa Física, Jurídica, Estrangeiro ou Trading
								1 – Pessoa Física / 2 – Pessoa Jurídica / 3 – Estrangeiro / 4 – Trading
						  </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Person"><xs:annotation><xs:documentation>Pessoa Física</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Company"><xs:annotation><xs:documentation>Pessoa Jurídica</xs:documentation></xs:annotation></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MarketSegment" type="MarketSegmentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Segmento de Mercado</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RegisterDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data da implantação do registro</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.data-implant</Field>
						   <Required>sim</Required>
						   <Type>date</Type>
						   <Length>99/99/9999</Length>
						   <Description>
								Data em que o Cliente/Fornecedor foi cadastrado no sistema
						  </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
			</xs:element>
			<xs:element name="RegisterSituation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Situação:
						Active 		Ativo
						Inactive 	Inativo
						Canceled 	Cancelado
						Pending		Pendente
						Suspended	Suspenso
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Active"><xs:annotation><xs:documentation>Ativo</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Inactive"><xs:annotation><xs:documentation>Inativo</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Canceled"><xs:annotation><xs:documentation>Cancelado</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Pending"><xs:annotation><xs:documentation>Pendente</xs:documentation></xs:annotation></xs:enumeration>
						<xs:enumeration value="Suspended"><xs:annotation><xs:documentation>Suspenso</xs:documentation></xs:annotation></xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Comments" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Observações</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="DATASUL">
						   <Field>emitente.observacoes</Field>
						   <Required>sim</Required>
						   <Type>string</Type>
						   <Length>2000</Length>
						   <Description>
								Informações adicionais relacionadas ao Cliente/Fornecedor
						  </Description>
						</FieldDocumentation>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
						</FieldDocumentation>
						<FieldDocumentation product="RM">
						</FieldDocumentation>
					</xs:appinfo>	
				</xs:annotation>
			</xs:element>
			<xs:element name="GovernmentalInformation" type="GovernmentalInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Datasul: Address = pessoa_jurid.nom_endereco(40), Number = ???, Complement = pessoa_jurid.nom_endereco_compl(10), CityCode = ???, CityName = pessoa_jurid.nom_cidade(32), District = pessoa_jurid.nom_bairro(20), State = pessoa_jurid.cod_unid_federac(3), Country = ??,ZIPCode = pessoa_jurid.cod_cep(20),Region=???</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShippingAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Datasul: Address = pessoa_jurid.nom_endereco(40), Number = ???, Complement = pessoa_jurid.nom_endereco_compl(10), CityCode = ???, CityName = pessoa_jurid.nom_cidade(32), District = pessoa_jurid.nom_bairro(20), State = pessoa_jurid.cod_unid_federac(3), Country = ??,ZIPCode = pessoa_jurid.cod_cep(20),Region=???</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ListOfCommunicationInformation" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CommunicationInformation" type="CommunicationInformationType" maxOccurs="unbounded" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Datasul: PhoneNumber= emitente.telefone ou .pessoa_jurid.cod_telefone ou pessoa_fisic.cod_telefone (15) PhoneExtension = emitente.ramal ou pessoa_jurid.cod_ramal ou pessoa_fisic.cod_ramal (5), FaxNumber = cont-emit.telefax ou contato.cod_fax_contat (15), FaxNumberExtension = cont-emit.ramal-fax ou contato.cod_ramal_fax_contat char (5), HomePage = emitente.home-page ou pessoa_jurid.nom_home_page ou pessoa_fisic.nom_home_page char (40), Email = emitente.e-mail ou pessoa_jurid.cod_e_mail ou pessoa_fisic.cod_e_mail char (40).</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ListOfContacts" minOccurs="0">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation></xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="Contact" type="ContactInformationType" maxOccurs="unbounded" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Datasul: Code = contato.num_pessoa_fisic (int), Title = cont-emit.cargo (20), Name = cont-emit.nome ou contato.nom_pessoa (40), Department = cont-emit.area (18), Address: (Address = pessoa_jurid.nom_endereco(40), Number = ???, Complement = pessoa_jurid.nom_endereco_compl(10), CityCode = ???, CityName = pessoa_jurid.nom_cidade(32), District = pessoa_jurid.nom_bairro(20), State = pessoa_jurid.cod_unid_federac(3), Country = ??,ZIPCode = pessoa_jurid.cod_cep(20),Region=???)</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ListOfBankingInformation" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BankingInformation" maxOccurs="unbounded" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="BankCode" type="xs:int" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Código do banco</xs:documentation>
											<xs:appinfo>
												<FieldDocumentation product="DATASUL">
													<Field>emitente.cod-banco</Field>
													<Required>sim</Required>
													<Type>integer</Type>
													<Length>999</Length>
													<Description>
														Código do banco junto a FEBRABAN
													</Description>
												</FieldDocumentation>
												<FieldDocumentation product="LOGIX">
												</FieldDocumentation>
												<FieldDocumentation product="PROTHEUS">
												</FieldDocumentation>
												<FieldDocumentation product="RM">
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="BankInternalId" type="xs:string" minOccurs="0" maxOccurs="1">
										<xs:annotation>
											<xs:documentation>InternalId do BankCode</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="BankName" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Nome do banco</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="30">
												</xs:maxLength>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="BranchCode" minOccurs="0">
										<xs:annotation>
											<xs:documentation></xs:documentation>
											<xs:appinfo>
												<FieldDocumentation product="DATASUL">
													<Field>emitente.agencia</Field>
													<Required>sim</Required>
													<Type>string</Type>
													<Length>8</Length>
													<Description>
														Agência bancária
													</Description>
												</FieldDocumentation>
												<FieldDocumentation product="LOGIX">
												</FieldDocumentation>
												<FieldDocumentation product="PROTHEUS">
												</FieldDocumentation>
												<FieldDocumentation product="RM">
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="8">
												</xs:maxLength>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="BranchKey" minOccurs="0">
										<xs:annotation>
											<xs:documentation></xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="10">
												</xs:maxLength>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CheckingAccountNumber" minOccurs="0">
										<xs:annotation>
											<xs:documentation></xs:documentation>
											<xs:appinfo>
												<FieldDocumentation product="DATASUL">
													<Field>emitente.conta-corren</Field>
													<Required>sim</Required>
													<Type>string</Type>
													<Length>20</Length>
													<Description>
														Conta corrente do emitente
													</Description>
												</FieldDocumentation>
												<FieldDocumentation product="LOGIX">
												</FieldDocumentation>
												<FieldDocumentation product="PROTHEUS">
												</FieldDocumentation>
												<FieldDocumentation product="RM">
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="20">
												</xs:maxLength>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CheckingAccountNumberKey" minOccurs="0">
										<xs:annotation>
											<xs:documentation></xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="2">
												</xs:maxLength>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CheckingAccountType" >										
										<xs:annotation>											
											<xs:documentation>Tipo da Conta Bancária do Fornecedor:
												1-Conta Corrente;
												2-Conta Poupança.
											</xs:documentation>											
											<xs:appinfo>
												<FieldDocumentation product="PROTHEUS">
													<Field>FIL_TIPCTA|A2_TIPCTA</Field>
													<Required>Nao</Required>
													<Type>String</Type>
													<Length>1</Length>
													<Description>
														Tipo de Conta Bancária a ser gravada na tabela de Contas do Fornecedor (FIL)
														Se for a conta principal, também salva a informação na tabela de Fornecedor (SA2).
													</Description>
												</FieldDocumentation>
												<FieldDocumentation product="TFS">
													<Field>id_tp_cta</Field>
													<Required>Nao</Required>
													<Type>String</Type>
													<Length>1</Length>
													<Description>Tipo de conta do cliente: "P"=Poupança; "C"=Conta Corrente </Description>
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="1"><xs:annotation><xs:documentation>Conta Corrente</xs:documentation></xs:annotation></xs:enumeration>
												<xs:enumeration value="2"><xs:annotation><xs:documentation>Conta Poupança</xs:documentation></xs:annotation></xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="MainAccount" >
										<xs:annotation>
											<xs:documentation>Conta Bancária Principal:
												1-Principal;
												2-Normal.
											</xs:documentation>
											<xs:appinfo>
												<FieldDocumentation product="PROTHEUS">
													<Field>FIL_TIPO</Field>
													<Required>Nao</Required>
													<Type>String</Type>
													<Length>1</Length>
													<Description>Indica se é uma conta principal ou uma conta normal, sendo 1=Principal e 2=Normal.</Description>
												</FieldDocumentation>
												<FieldDocumentation product="TFS">
													<Field>nr_seq_ref</Field>
													<Required>Nao</Required>
													<Type>Integer</Type>
													<Length>10</Length>
													<Description>Numero sequencial da conta do cliente, onde 1 = Principal e maior que 1 = Normal</Description>
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="1"><xs:annotation><xs:documentation>Principal</xs:documentation></xs:annotation></xs:enumeration>
												<xs:enumeration value="2"><xs:annotation><xs:documentation>Normal</xs:documentation></xs:annotation></xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="CurrencyAccount" type="xs:string">
										<xs:annotation>
											<xs:documentation>Moeda utilizada para os processos com a conta bancária.</xs:documentation>
											<xs:appinfo>
												<FieldDocumentation product="PROTHEUS">
													<Field>FIL_MOEDA</Field>
													<Required>Nao</Required>
													<Type>Integer</Type>
													<Length>2</Length>
													<Description>Moeda utilizada para os processos com a conta bancária.</Description>
												</FieldDocumentation>
												<FieldDocumentation product="TFS">
													<Field>Nao se aplica</Field>
													<Required>Nao</Required>
													<Type>Nao se aplica</Type>
													<Length>Nao se aplica</Length>
													<Description>É enviado o valor fixo "01" para a moeda utilizada nos processos com a conta bancária.</Description>
												</FieldDocumentation>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BillingInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BillingCustomerCode"
							type="xs:int" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Datasul: BillingCustomerCode =
									emitente.cod-cobranca ou
									pessoa_jurid.num_pessoa_jurid_cobr
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BillingCustomerInternalId"
							type="xs:string" maxOccurs="1" minOccurs="0">
						</xs:element>
						<xs:element name="Address" type="AddressType"
							minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Datasul: Address =
									pessoa_jurid.nom_ender_cobr(40),
									Number = ???, Complement =
									pessoa_jurid.nom_ender_cobr_compl(10),
									CityCode = ???, CityName =
									pessoa_jurid.nom_cidad_cobr(32),
									District =
									pessoa_jurid.nom_bairro_cobr(20),
									State =
									pessoa_jurid.cod_unid_federac_cobr(3),
									Country = ??,ZIPCode =
									pessoa_jurid.cod_cep_cobr(20),Region=???
									ou Address =
									emitente.endereco-cob(40), Number =
									???, Complement = ???, CityCode =
									???, CityName =
									emitente.cidade-cob(25), District =
									emitente.bairro-cob(30), State =
									emitente.estado-cobr(4), Country =
									emitente.pais-cob(20),ZIPCode =
									emitente.cep-cob(12),Region=???
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="VendorInformation" type="VendorInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:element>
